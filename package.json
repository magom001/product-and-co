{
    "name": "product-and-co",
    "version": "1.0.0",
    "description": "Test assignment",
    "scripts": {
        "build": "npx webpack --env.production",
        "start": "PORT=8080 npx ts-node ./server/index.ts",
        "build:server": "npx tsc ./server/index.ts",
        "build:watch": "npx webpack --profile --colors --watch",
        "server:start": "npx nodemon",
        "dev": "concurrently \"npm run build:watch\" \"npm run server:start\"",
        "ci:server": "PORT=9009 node ./server.js &",
        "ci:run": "npm run build && npm run ci:server && wait-on http://localhost:9009 && npx jest"
    },
